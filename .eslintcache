        <div class=\"home-images\">\n                            <a href=\"https://www.bryange.com/about\" rel=\"noreferrer\"><img src={bottomimage} className=\"home-image\" alt=\"Home Profile Pic\" /></a>\n                            <a href=\"https://www.bryange.com/about\" rel=\"noreferrer\"><Animation><img src={topcursor} className=\"home-image-cursor top-image\" alt=\"Figma Cursor\" /></Animation></a>\n                        </div>\n                        {/* {props.animated && <h2 className=\"display-4\"><Animation>{props.animated}</Animation></h2>} */}\n                        {props.title && <h2 className=\"display-5 font-weight-bolder home-title\">{props.title}</h2>}\n                        {/* {props.subTitle && <h2 className=\"display-5 font-weight-normal\">{props.subTitle}</h2>} */}\n                        <div className=\"home-positions\">\n                            <h3 className=\"font-weight-normal role\">UX/UI Design Intern at <a className=\"link-underline\" href=\"https://www.cargill.com/\" target=\"_blank\" rel=\"noreferrer\">Cargill</a></h3>\n\n                            <h3 className=\"font-weight-normal role\">National Operations Strategist at <a className=\"link-underline\" href=\"https://hack4impact.org/\" target=\"_blank\" rel=\"noreferrer\">Hack4Impact</a></h3>\n\n                            <h3 className=\"font-weight-normal role\">Rising Sophomore at UIUC studying <a className=\"link-underline\" href=\"https://cs.illinois.edu/academics/undergraduate/degree-program-options/bs-statistics-computer-science\" target=\"_blank\" rel=\"noreferrer\">CS + Statistics</a></h3>\n                        </div>\n                    </Col>\n                </Row>\n            </Container>\n            {/* </Jumbotron> */}\n\n            <Grid projects={props.projects} />\n        </FadeIn >\n    );\n}\n\nexport default HomePage;","/Users/bryange/Documents/Websites/react-portfolio/src/pages/ResumePage.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/Footer.js",["127","128","129","130","131","132"],"import React from 'react';\n\nfunction Footer() {\n    return(\n        <footer className=\"sticky-footer my-gray background-med\">\n            <ul>\n                <li><a href=\"mailto:bryanjg2@illinois.edu\" target=\"_blank\" rel=\"noreferrer\" className=\"default\" alt=\"Email\"><i class=\"fa fa-envelope-o\" aria-hidden=\"true\"></i></a></li>\n                <li><a href=\"https://linkedin.com/in/bryange\" target=\"_blank\" rel=\"noreferrer\" className=\"default\" alt=\"LinkedIn\"><i class=\"fa fa-linkedin\" aria-hidden=\"true\"></i></a></li>\n                <li><a href=\"https://github.com/bryange3\" target=\"_blank\" rel=\"noreferrer\" className=\"default\" alt=\"GitHub\"><i class=\"fa fa-github\" aria-hidden=\"true\"></i></a></li>\n                <li><a href=\"https://instagram.com/bryangephoto\" target=\"_blank\" rel=\"noreferrer\" className=\"default\" alt=\"Instagram\"><i class=\"fa fa-instagram\" aria-hidden=\"true\"></i></a></li>\n                <li><a href=\"https://twitter.com/omgebryan\" target=\"_blank\" rel=\"noreferrer\" className=\"default\" alt=\"Twitter\"><i class=\"fab fa-twitter\" aria-hidden=\"true\"></i></a></li>\n                <li><a href=\"https://open.spotify.com/user/omgebryan?si=AzsYGAQnQmS8m6Jlg-cUMw\" target=\"_blank\" rel=\"noreferrer\" className=\"default\" alt=\"Spotify\"><i class=\"fab fa-spotify\" aria-hidden=\"true\"></i></a></li>\n            </ul>\n            This site was designed and coded with ‚ù§Ô∏è using React.\n            <br />¬© Bryan Ge 2021\n        </footer>\n    )\n}\n\nexport default Footer;","/Users/bryange/Documents/Websites/react-portfolio/src/components/Hero.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/Content.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/Grid.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/Card.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/CardInfo.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/ScrollArrow.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/ProjectTitle.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/pages/PlayPage.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/Navbar/Navbar.js",[],["133","134"],"/Users/bryange/Documents/Websites/react-portfolio/src/components/Navbar/MenuItems.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/components/Button.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/pages/Projects/Fun/SafeWalksPage.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/pages/Projects/Work/MiningForBiasPage.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/pages/Projects/Fun/TaskCalPage.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/pages/Projects/Work/IotPage.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/pages/Projects/Work/TrillPage.js",["135"],"import React from 'react';\nimport Content from '../../../components/Content.js';\nimport ProjectTitle from '../../../components/ProjectTitle.js';\nimport grouppic from '../../../assets/project/trill/group.png';\n\nfunction TrillPage(props) {\n    window.scrollTo(0, 0);\n    const width = 7;\n    return (\n        <div>\n            <ProjectTitle info={props.info} width={width} />\n            <Content width={width}>\n                <body>\n                    <p>\n                        <b>Role</b>: {props.info.roles}\n                        <br />\n                        <b>Timeline</b>: {props.info.timeline}\n                        <br />\n                        <b>Design Tools</b>: Figma, Procreate\n                    </p>\n\n                    <hr class=\"rounded\" />\n\n                    <p>\n                        As a Design Intern for Trill Project, I developed features for the mobile app, revamped the Trill Project website, and created graphics for partnerships. My work consisted of ideation, wireframing, and prototyping new mobile app features, as well as conducting user research and developing beta tester relations.\n                    </p>\n\n                    <p>\n                        I lead the creation of designs used for Mental Wealth Month, a campaign done in collaboration with Tumblr. These designs were displayed on the top of Tumblr's feed and used to promote awareness for mental health and relevant apps.\n                    </p>\n\n                    <p>\n                        Here is the website I designed in Webflow as part of this campaign:\n                    </p>\n\n                    <iframe className=\"project-img center\" width=\"500\" height=\"500\" src=\"https://www.youtube-nocookie.com/embed/29cF85m_jjE?autoplay=1&mute=1&loop=1&playlist=29cF85m_jjE\" title=\"YouTube video player\" frameborder=\"0\" allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen></iframe>\n\n                    <br />\n                    <p>\n                        For more details, contact me at <a href=\"mailto:bryanjg2@illinois.edu\" target=\"_blank\" rel=\"noreferrer\" className=\"default\" alt=\"Email\">bryanjg2@illinois.edu</a>!\n                    </p>\n\n                    <hr class=\"rounded\" />\n\n                    <img src={grouppic} alt=\"Trill Group Photo\" className=\"captionedImg center\" />\n                    <p className=\"image-caption\">\n                        Here's a group pic from our final meeting! Will definitely miss this team. :,)\n                    </p>\n                </body>\n            </Content>\n        </div>\n    );\n}\n\nexport default TrillPage;","/Users/bryange/Documents/Websites/react-portfolio/src/pages/Projects/Work/NourPage.js",["136"],"import React from 'react';\n\nimport Content from '../../../components/Content.js';\nimport ProjectTitle from '../../../components/ProjectTitle.js';\n\nimport accessibilityiterations from '../../../assets/project/nour/AccessibilityIterations.png';\nimport accessibilitypreferences from '../../../assets/project/nour/AccessibilityPreferences.png';\nimport loginiterations from '../../../assets/project/nour/LoginIterations.png';\nimport onboarding from '../../../assets/project/nour/Onboarding.png';\nimport roomdiscovery from '../../../assets/project/nour/RoomDiscovery.png';\nimport roomdiscoveryvariations from '../../../assets/project/nour/RoomDiscoveryVariations.png';\nimport stats from '../../../assets/project/nour/stats.png';\nimport team from '../../../assets/project/nour/team.png';\nimport searchmethods from '../../../assets/project/nour/SearchMethods.png';\n\n\nfunction NourPage(props) {\n    window.scrollTo(0, 0);\n    const width = 6;\n    return (\n        <div>\n            <ProjectTitle info={props.info} width={width} />\n            <Content width={width}>\n                <body>\n                    <p>\n                        <b>Role</b>: {props.info.roles}\n                        <br />\n                        <b>Timeline</b>: {props.info.timeline}\n                        <br />\n                        <b>Team Members</b>: {props.info.teamMembers}\n                        <br />\n                        <b>Design Tools</b>: Figma\n                        <br />\n                        <br />\n                        Our project received the following prizes for the 2021 HackDuke Ideate Designathon:\n                        <ul>\n                            <li>\n                                1st Place for Huawei Innovation Awards\n                            </li>\n                            <li>\n                                1st Place for Caring House Challenge\n                            </li>\n                        </ul>\n                    </p>\n\n                    <hr class=\"rounded\" />\n\n                    <p className=\"paragraph\">\n                        <p className=\"section\">\n                            Challenge\n                        </p>\n\n                        <h3 className=\"subsection\">\n                            Create your own design solution to aid the Caring House mission of providing comfortable, supportive, and affordable housing to Duke Cancer Institute adult outpatients and their caregivers.\n                        </h3>\n                        Your problem space can be anything related to Caring House or similar fields like cancer patients, medical support communities, etc.\n                    </p>\n\n                    <p className=\"paragraph\">\n                        <p className=\"section\">\n                            Identified Problem\n                        </p>\n\n                        <h3 className=\"subsection\">\n                            Patients in elder homes feel isolated and socially out of touch.\n                        </h3>\n                    </p>\n\n                    <p className=\"paragraph\">\n                        <p className=\"section\">\n                            Solution\n                        </p>\n\n                        <h3 className=\"subsection\">\n                            Nour is a mobile video call platform that brings the elderly together based on their interests.\n                        </h3>\n                    </p>\n\n                    <hr class=\"rounded\" />\n\n                    <p className=\"paragraph\">\n                        <p className=\"section\">\n                            Assumption\n                        </p>\n\n                        <h3 className=\"subsection\">\n                            In this project, we assume that our target audience has some understanding of how to use and navigate technology.\n                        </h3>\n                        Because of the pandemic and social distancing, we decided to create a mobile app to host our social platform. Since our target audience is not as familiar with technology and can find understanding new technologies difficult, we focused on designing the most comprehensible and accessible experience possible.\n                    </p>\n\n                    <hr class=\"rounded\" />\n\n                    <p className=\"section\">\n                        Key Features\n                    </p>\n\n                    <p className=\"paragraph impact-statement\">\n                        üëâ Designing all the visual elements to be as accessible as possible was our top priority. I chose to use large text, contrasting colors, and labels that our target audience would be able to easily understand. I also brought this approach to the various user experiences throughout the app as detailed below.\n                    </p>\n\n                    <p className=\"paragraph\">\n                        <h3 className=\"subsection\">\n                            Simple and Efficient Onboarding\n                        </h3>\n\n                        Typical login processes involve:\n                        <ul>\n                            <li>\n                                Memorizing login information\n                            </li>\n                            <li>\n                                Typing out email + password\n                            </li>\n\n                        </ul>\n                        I chose to use a profile code scanning system. <mark>Our users will not need to remember their login details and will only need to point their smartphone camera at their profile code</mark> provided by their caregivers.\n\n                        <img src={onboarding} alt=\"Simple and Efficient Onboarding\" className=\"project-img\" />\n                    </p>\n\n                    <p className=\"paragraph\">\n                        <h3 className=\"subsection\">\n                            Accessibility Preferences\n                        </h3>\n\n                        When designing our \"Clubhouse for Elders\", <mark>we focused on accessibility and understandability</mark>. Since our target audience is seniors and our goal is to connect those within various medical support communities, making Nour easy-to-use and intuitive was our largest challenge. I chose to design <mark>accessibility preferences for those with visual and hearing difficulties</mark>, making our platform more inclusive than other competitors.\n\n                        <img src={accessibilitypreferences} alt=\"Accessibility Preferences\" className=\"project-img\" />\n                    </p>\n\n                    <p className=\"paragraph\">\n                        <h3 className=\"subsection\">\n                            Video / Voice Call Room Discovery\n                        </h3>\n\n                        Users can discover and preview rooms one at a time. While browsing the various rooms, the <mark>audio from inside the room is played aloud so users can decide whether or not they want to hop in and join the conversation</mark>.\n\n                        <img src={roomdiscovery} alt=\"Call Room Discovery\" className=\"project-img-smaller center\" />\n                    </p>\n\n                    <p className=\"paragraph\">\n                        <h3 className=\"subsection\">\n                            Type & Voice Search Methods\n                        </h3>\n\n                        I also decided to implement voice search alongside the traditional type search method for those who may find it easier to speak what type of video call room they would like to join.\n\n                        <img src={searchmethods} alt=\"Search Methods\" className=\"project-img-small center\" />\n                    </p>\n\n                    <hr class=\"rounded\" />\n\n                    <p className=\"paragraph\">\n                        <p className=\"section\">\n                            Research\n                        </p>\n\n                        <h3 className=\"subsection\">\n                            I called local home care facilities and read online sources when defining our problem space.\n                        </h3>\n\n                        <img src={stats} alt=\"Research Statistics\" className=\"project-img-medium center\" />\n\n                        Based on our research, we decided to <mark>tackle the lack of social interaction that many home care patients face</mark>. Ideating a solution for this problem was challenging and took lots of brainstorming. We saw the rise of Clubhouse, an audio chatroom social network, in connecting people virtually, so we decided to make a version for elders that specifically focused on accessibility.\n                    </p>\n\n                    <p className=\"paragraph\">\n                        <p className=\"section\">\n                            Iteration\n                        </p>\n\n                        <h3 className=\"subsection\">\n                            After our first iteration of wireframes, I Zoom called my parents, shared my screen, and gave them remote control to walk through our prototype.\n                        </h3>\n\n                        Since we only had 24 hours for this challenge, <mark>my parents served as the closest in age group and generation to our target audience</mark>. I let my parents click away at our prototype and discovered certain areas of confusion. From this, I iterated and produced more understandable screens:\n\n                        <h3 className=\"subsubsection\">\n                            1. Improved communication of profile code scan\n                        </h3>\n\n                        My dad mentioned his confusion with what to do at the \"login / profile code scanning\" screen. I realized that I needed to make a <mark>clear indicator of the profile code being scanned</mark>.\n\n                        <img src={loginiterations} alt=\"Login Iterations\" className=\"project-img\" />\n\n                        <h3 className=\"subsubsection\">\n                            2. Clarity of accessibility preferences\n                        </h3>\n\n                        Devan and I compared the two variants of the accessibility preferences progression and chose the accessibility preferences progression on the right of the image below. Although more screens causes a longer progression and setup process, it provides <mark>more clarity on what the preferences (VoiceOver and closed captioning) mean</mark>. By separating the preferences into multiple screens, there is <mark>much less clutter on the interface</mark>, leading to <mark>easier comprehension of the visual components</mark>.\n\n                        <img src={accessibilityiterations} alt=\"Accessibility Preference Iterations\" className=\"project-img\" />\n\n                        <h3 className=\"subsubsection\">\n                            3. Less overwhelming room discovery\n                        </h3>\n\n                        I chose to have our users <mark>browse through rooms one at a time so that the interface would be less overwhelming</mark> to view and navigate. By giving a user one room to preview at a time, they would also be able to <mark>listen in on what discussions are taking place before deciding to enter the room</mark> or not.\n\n                        <p className=\"paragraph\">\n                            By having the <mark>\"next\" and \"previous\" room buttons below the room card, they can be easier labeled and tapped</mark>. The larger and clearly labeled buttons underneath the room card also <mark>allow the room card to display more text if the room topic text is longer</mark>.\n                        </p>\n\n                        <img src={roomdiscoveryvariations} alt=\"Room Discovery Variations\" className=\"project-img\" />\n                    </p>\n\n                    <hr className=\"rounded\" />\n\n                    <p className=\"paragraph\">\n                        <p className=\"section\">\n                            Next Steps\n                        </p>\n\n                        Our next steps for Nour involve <mark>testing our app on patients of elderly homes</mark>. I plan on conducting <mark>usability tests to learn how our target audience navigates the app</mark>. I will also work with the home care facilities to conduct <mark>user interviews to learn more about our users' pain points with the app</mark> and any wants that are not being fulfilled by Nour.\n\n                        <p className=\"paragraph\">\n                            We also hope to create a <mark>backend system that allows workers at different care centers to communicate and set up timely events</mark> (games, shows, etc.). Implementing games such as Bingo or Trivia within the app and expanding to an iPad app is also something we hope to reach in the future. With all these feature updates, accessibility will be our largest priority. Designing entertainment features that are easy to understand and navigate is the largest but most necessary challenge.\n                        </p>\n                    </p>\n\n                    <p className=\"paragraph\">\n                        <p className=\"section\">\n                            What I Learned\n                        </p>\n\n                        <h3 className=\"subsection\">\n                            Accessible design is challenging but necessary.\n                        </h3>\n\n                        This was the first time that I was given a design prompt where I knew almost nothing about the target audience. Building an app for people who are naturally unfamiliar with technology proved difficult. However, through online research and Medium articles, I gained some knowledge about accessible design.\n\n                        <p className=\"paragraph\">\n                            I certainly have a lot more to learn regarding designing for all and designing inclusively, but I'm glad that this designathon gave me an introduction to it. Thank you to all the mentors who provided me with their input during the ideation process and to my team for being amazing!                        </p>\n                    </p>\n\n                    <img src={team} alt=\"Team\" className=\"project-img\" />\n\n                    <br />\n\n                    <p className=\"paragraph\">\n                        Check out our <a href=\"https://www.figma.com/file/o2siBMfCFww1ksk5Xz7ZE5/HackDuke-Ideate-2021-Nour?node-id=0%3A1\" target=\"_blank\" rel=\"noreferrer\" className=\"link-underline\">Figma document</a> and play around with the prototype below!\n                    </p>\n\n                    <iframe stitle=\"IOT Playground Prototype\" className=\"full-width\" style={{ border: \"1px solid rgba(0, 0, 0, 0.1)\" }} width=\"800\" height=\"700\" src=\"https://www.figma.com/embed?embed_host=share&url=https%3A%2F%2Fwww.figma.com%2Fproto%2Fo2siBMfCFww1ksk5Xz7ZE5%2FHackDuke-Ideate-2021-Nour%3Fnode-id%3D1%253A3%26viewport%3D258%252C261%252C0.10952381789684296%26scaling%3Dscale-down%26page-id%3D0%253A1\" allowfullscreen></iframe>\n                </body>\n            </Content>\n        </div>\n    );\n}\n\nexport default NourPage;","/Users/bryange/Documents/Websites/react-portfolio/src/pages/Projects/Work/LeafPage.js",[],"/Users/bryange/Documents/Websites/react-portfolio/src/pages/Projects/Work/DasPage.js",[],{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","replacedBy":"140"},{"ruleId":"141","severity":1,"message":"142","line":36,"column":8,"nodeType":"143","messageId":"144","endLine":36,"endColumn":21},{"ruleId":"141","severity":1,"message":"145","line":42,"column":8,"nodeType":"143","messageId":"144","endLine":42,"endColumn":19},{"ruleId":"141","severity":1,"message":"146","line":44,"column":8,"nodeType":"143","messageId":"144","endLine":44,"endColumn":16},{"ruleId":"141","severity":1,"message":"147","line":3,"column":8,"nodeType":"143","messageId":"14